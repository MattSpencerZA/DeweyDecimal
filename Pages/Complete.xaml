<Page x:Class="Dewey_Training.Pages.Complete"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Dewey_Training.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="707" d:DesignWidth="1050"
      Title="Complete">

    <Grid>
        <Label Content="The Books Have Been Successfully Ordered!" Foreground="White" FontFamily="Rockwell" FontSize="35" HorizontalContentAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Top" Height="55" Width="800" Margin="0,257,0,0"/>
        <Label Content="Time Taken" x:Name="timeTaken" Foreground="White" FontFamily="Rockwell" FontSize="25" HorizontalContentAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" Height="40" Width="800"/>
        <Button x:Name="ViewOrder" Content="View Order" HorizontalAlignment="Left" Margin="884,649,0,0" VerticalAlignment="Top" Width="141" Height="48" FontSize="16" TabIndex="1" FontFamily="Rockwell" FontWeight="Bold" Click="ViewOrder_Click"/>
        <Button x:Name="Finish" Content="Finish" HorizontalAlignment="Left" Margin="712,649,0,0" VerticalAlignment="Top" Width="141" Height="48" FontSize="16" TabIndex="1" FontFamily="Rockwell" FontWeight="Bold" Click="Finish_Click"/>
    </Grid>
</Page>
